{
  "expo": {
    "name": "ReactNativeMessager",
    "icon": "./src/assets/icons/icon.png",
    "version": "0.1.0",
    "slug": "ReactNativeMessager",
    "sdkVersion": "33.0.0",
    "privacy": "public",
    "orientation": "portrait",
    "githubUrl": "https://github.com/andyc365/ReactNativeMessager",
    "ios": {
      "bundleIdentifier": "com.andyc.messager"
    },
    "android": {
      "package": "com.andyc.messager",
      "versionCode": 17
    },
    "androidStatusBar": {
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "src/assets/images/source/*",
      "src/assets/icons/**/*.png",
      "src/assets/fonts"
    ],
    "splash": {
      "image": "src/assets/images/source/splash.png",
      "resizeMode": "contain"
    },
    "platforms": [
      "android",
      "ios",
      "web"
    ]
  }
}